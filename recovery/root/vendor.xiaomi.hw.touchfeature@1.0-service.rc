service touch_report /vendor/bin/touch_report
    class hal
    user root
    group root system uhid input
    capabilities SYS_NICE
    ioprio rt 4
    disable
    seclabel u:r:recovery:s0

service touchsensor /vendor/bin/touchsensor
    class hal
    user root
    group root system uhid input
    disable
    seclabel u:r:recovery:s0
